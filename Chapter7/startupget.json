[
  {
    "description":"模拟一个没有参数的get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "headers": {
        "Content-Type": "GBK"
      },
      "text": "这是没有参数的get响应数据"
    }
  },
  {
    "description":"模拟一个有参数的get请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "GBK"
      },
      "text": "我胡汉三回来了！！！！"
    }
  }
]